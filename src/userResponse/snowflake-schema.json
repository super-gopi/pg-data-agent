  "commonQueries": [
    {
      "description": "Total revenue",
      "query": "SELECT SUM(O_TOTALPRICE) as total_revenue FROM TPCH_SF10.ORDERS"
    },
    {
      "description": "Revenue by month",
      "query": "SELECT DATE_TRUNC('month', O_ORDERDATE) as month, SUM(O_TOTALPRICE) as revenue FROM TPCH_SF10.ORDERS GROUP BY month ORDER BY month"
    },
    {
      "description": "Orders by status",
      "query": "SELECT O_ORDERSTATUS, COUNT(*) as count FROM TPCH_SF10.ORDERS GROUP BY O_ORDERSTATUS"
    },
    {
      "description": "Top customers by spending",
      "query": "SELECT c.C_NAME, SUM(o.O_TOTALPRICE) as total_spent FROM TPCH_SF10.CUSTOMER c JOIN TPCH_SF10.ORDERS o ON c.C_CUSTKEY = o.O_CUSTKEY GROUP BY c.C_NAME ORDER BY total_spent DESC LIMIT 10"
    },
    {
      "description": "Customers by market segment",
      "query": "SELECT C_MKTSEGMENT, COUNT(*) as count FROM TPCH_SF10.CUSTOMER GROUP BY C_MKTSEGMENT"
    }
  ],
